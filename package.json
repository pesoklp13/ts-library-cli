{
  "name": "ts-library-cli",
  "version": "0.0.1",
  "description": "CLI to build your own typescript library",
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "create-ts-library": "./bin/ts-library.js",
    "create-ts-react-component-library": ""
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.(ts|tsx)' 'test/**/*.(ts|tsx)'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src"
  },
  "keywords": [
    "typescript",
    "cli",
    "library",
    "rollup",
    "typedoc"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Peter Sokol√≠k",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.19.0"
  }
}
